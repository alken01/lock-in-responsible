{
  "version": 1,
  "canisters": {
    "lock_in_backend": {
      "type": "motoko",
      "main": "src/lock_in_backend/main.mo"
    },
    "lock_in_frontend": {
      "type": "assets",
      "source": ["frontend/dist"],
      "dependencies": ["lock_in_backend"]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  }
}
